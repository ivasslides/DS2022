import boto3

s3 = boto3.client('s3', region_name='us-east-1')

response = s3.list_buckets()

for r in response['Buckets']:
    print(r['Name'])

bucket = 'ds2022-fbv2sc'
local_file = 'project/sailboat.png'

resp = s3.put_object(
    Body = local_file,
    Bucket = bucket,
    Key = local_file
)

file1 = 'project/lasers.jpg'

resp1 = s3.put_object(
    Body = file1,
    Bucket = bucket,
    Key = file1
)
